import java.util.Vector;

public class Swarmodon extends Unit{
	
	private Overmind boss;
	private Vector<Swarmide> children;
	
	Swarmodon()
	{
		super();
		children = new Vector<Swarmide>();
	}
	
	public boolean canProduceSwarmide()
	{
		if (children.size() < 5)
			return true;
		else
			return false;
	}

}
